# Frontend Environment Configuration
# Copy this file to .env and update the values

# API Configuration
REACT_APP_API_URL=http://localhost:8000/api/v1
REACT_APP_WS_URL=ws://localhost:8000/ws

# Application Settings
REACT_APP_NAME=DSA Code Visualizer Pro
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_AI_ANALYSIS=true
REACT_APP_ENABLE_CODE_EXECUTION=true
REACT_APP_ENABLE_VISUALIZATION=true
REACT_APP_ENABLE_COMPLEXITY_ANALYSIS=true
REACT_APP_ENABLE_REAL_TIME_UPDATES=true

# Analytics and Monitoring
REACT_APP_GA_TRACKING_ID=your-ga-tracking-id
REACT_APP_SENTRY_DSN=your-sentry-dsn
REACT_APP_ANALYTICS_ENABLED=true

# External Services
REACT_APP_GITHUB_CLIENT_ID=your-github-client-id
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id

# Performance Settings
REACT_APP_CACHE_TTL=3600
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_REQUEST_TIMEOUT=30000

# Development Settings
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=info
REACT_APP_ENABLE_HOT_RELOAD=true

# Production Settings
REACT_APP_ENABLE_COMPRESSION=true
REACT_APP_ENABLE_SERVICE_WORKER=true
REACT_APP_ENABLE_PWA=true 